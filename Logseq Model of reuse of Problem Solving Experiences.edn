       :block/format :markdown,
       :block/children
       [{:block/id #uuid "6479df46-45e3-491d-8ef1-c87831db32ae",
         :block/properties nil,
         :block/format :markdown,
         :block/children [],
         :block/content ""}
        {:block/id #uuid "6479df46-183b-4b42-bd52-5101061dbb3c",
         :block/properties
         {:action-responsible "___",
          :result "___",
          :priority #{"High" "Medium" "Low"},
          :deadline "___",
          :progress #{"ActionPlanified" "WorkInProgress" "WorkDone"},
          :completion-date "___",
